<catalog xmlns="http://scap.nist.gov/schema/oscal">
  <title>OSCAL refinement layer</title>
  <declarations></declarations>
  <control type="document-constraint">
    <prop role="scope">level</prop>
    <prop role="inclusive-maximum">10</prop>
    <prop role="inclusive-minimum">0</prop>
    <prop role="datatype">decimal</prop>
    <desc>
      <p>Property 'level' must be between 0 and 10, inclusive.</p>
    </desc>
  </control>
  
  <control type="schematron-rule">
    <prop role="match">prop[@role='level']</prop>
    <prop role="report-when">. &gt; 10</prop>
    <desc>
      <p>Property 'level' must be 10 or less.</p>
    </desc>
  </control>
  <control type="schematron-rule">
    <prop role="match">prop[@role='level']</prop>
    <prop role="report-when">. &lt; 0</prop>
    <desc>
      <p>Property 'level' must be 0 or more.</p>
    </desc>
  </control>
  
</catalog>