<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/css" href="../lib/oscal.css"?>
<?xml-model href="../lib/oscal-working.rnc" type="application/relax-ng-compact-syntax"?>
<?xml-model href="../lib/oscal-consistency.sch" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<declarations xmlns="http://scap.nist.gov/schema/oscal">
  <statement context="SP800-53" class="description">
    <singleton/>
    <required/>
  </statement>
  <property context="SP800-53" class="control-class">
    <singleton/>
    <required/>
    
      <value>Technical</value>
      <value>Operational</value>
      <value>Management</value>
   </property>
   <property context="SP800-53" class="priority">
    <singleton/>
     <value>P1</value>
      <value>P2</value>
      <value>P0</value>
      <value>P3</value>
   </property>
  <statement context="SP800-53" class="guidance"/>
  <property context="SP800-53" class="number">
    <singleton/>
       <required/>
     <regex>^(AC|AT|AU|CA|CM|CP|IA|IR|MA|MP|PE|PL|PM|PS|RA|SA|SC|SI)[\d\.\-]*$</regex>
  </property>
   <property context="SP800-53" class="baseline-impact">
    <singleton/>
      <value>HIGH</value>
   </property>

  <property context="SP800-53-enhancement" class="number">
    <singleton/>
    <!--<required/>-->
    <value><inherit/><autonum>(1)</autonum></value>
  </property>
  
  <property context="assessment" class="method">
    <singleton/>
      <required/>
      <value>EXAMINE</value>
      <value>INTERVIEW</value>
      <value>TEST</value>
  </property>
  
  <statement context="SP800-53-enhancement" class="guidance">
    <singleton/>
  </statement>

  <property context="SP800-53-enhancement" class="baseline-impact">
    <singleton/>
      <value>HIGH</value>
      <value>MODERATE</value>
   </property>
  
  <property context="objective" class="name">
    <singleton/>
    <required/>
    <!--<value><inherit from="number"/><autonum>.1</autonum></value>-->
  </property>
  
  <property context="decision" class="name">
    <singleton/>
    <required/>
    <value><inherit from="name"/> <autonum>(i)</autonum></value>
  </property>
  
  <statement context="SP800-53-enhancement decision" class="description">
    <singleton/>
    <required/>
  </statement>

  <property context="assessment" class="object">
    <required/>
  </property>
  
  
</declarations>
